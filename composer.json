{
    "name": "loevgaard/cron-bundle",
    "type": "symfony-bundle",
    "description": "A cron bundle for Symfony",
    "keywords": [
        "symfony",
        "bundle",
        "symfony-bundle"
    ],
    "homepage": "https://github.com/loevgaard/cron-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim Loevgaard",
            "email": "joachim@loevgaard.dk",
            "homepage": "https://github.com/loevgaard",
        }
    ],
    "require": {
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\CronBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loevgaard\\CronBundle\\": ""
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}

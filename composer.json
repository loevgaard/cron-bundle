{
    "name": "loevgaard/cron-bundle",
    "type": "symfony-bundle",
    "description": "A cron bundle for Symfony",
    "keywords": [
        "symfony",
        "bundle",
        "symfony-bundle"
    ],
    "homepage": "https://github.com/loevgaard/cron-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim Loevgaard",
            "email": "joachim@loevgaard.dk",
            "homepage": "https://github.com/loevgaard"
        }
    ],
    "require": {
        "php": "~7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "mtdowling/cron-expression": "^1.2",
        "symfony/framework-bundle": "^3.0",
        "symfony/process": "^3.3",
        "symfony/validator": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "symfony/phpunit-bridge": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\CronBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Loevgaard\\CronBundle\\": ""
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
